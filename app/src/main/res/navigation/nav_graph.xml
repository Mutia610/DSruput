<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/loginFragment">

    <fragment
        android:id="@+id/loginFragment"
        android:name="com.mutia.dsruput.view.LoginFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_loginFragment_to_outletFragment"
            app:destination="@id/outletFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.mutia.dsruput.view.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" />
    <fragment
        android:id="@+id/outletFragment"
        android:name="com.mutia.dsruput.view.OutletFragment"
        android:label="fragment_outlet"
        tools:layout="@layout/fragment_outlet" />
</navigation>